<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_booking_popup_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    android:focusable="true"
    android:focusableInTouchMode="true">

    <RelativeLayout
        android:id="@+id/popup_text_table_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true">
        <de.morrox.fontinator.FontTextView
            android:id="@+id/date_title"
            android:layout_width="154dp"
            android:layout_height="37dp"
            android:background = "@drawable/regular_state"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:text="날짜"
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Regular.otf" />
        <de.morrox.fontinator.FontEditText
            android:id="@+id/date_text"
            android:layout_width="360dp"
            android:layout_height="37dp"
            android:background = "@drawable/regular_state"
            android:layout_toRightOf="@+id/date_title"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:singleLine="true"
            android:inputType="date|text"
            android:text=""
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Regular.otf" />

        <de.morrox.fontinator.FontTextView
            android:id="@+id/team_title"
            android:layout_width="154dp"
            android:layout_height="37dp"
            android:layout_below="@+id/date_title"
            android:background = "@drawable/regular_state"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:text="팀명"
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Medium.otf" />

        <de.morrox.fontinator.FontEditText
            android:id="@+id/team_text"
            android:layout_width="360dp"
            android:layout_height="37dp"
            android:layout_below="@+id/date_text"
            android:layout_toRightOf="@+id/team_title"
            android:background="@drawable/regular_state"
            android:gravity="center_horizontal|left"
            android:includeFontPadding="false"
            android:paddingLeft="20dp"
            android:singleLine="true"
            android:inputType="text"
            android:text=""
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Regular.otf" />

        <de.morrox.fontinator.FontTextView
            android:id="@+id/booking_personnel_title"
            android:layout_width="154dp"
            android:layout_height="37dp"
            android:layout_below="@+id/team_title"
            android:background = "@drawable/regular_state"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:text="예약자"
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Medium.otf" />
        <de.morrox.fontinator.FontEditText
            android:id="@+id/booking_personnel_text"
            android:layout_width="360dp"
            android:layout_height="37dp"
            android:background = "@drawable/regular_state"
            android:layout_below="@+id/team_text"
            android:layout_toRightOf="@+id/booking_personnel_title"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:singleLine="true"
            android:inputType="text"
            android:text=""
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Regular.otf" />

        <de.morrox.fontinator.FontTextView
            android:id="@+id/booking_phone_title"
            android:layout_width="154dp"
            android:layout_height="37dp"
            android:layout_below="@+id/booking_personnel_title"
            android:background = "@drawable/regular_state"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:text="예약자전화번호"
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Medium.otf" />
        <de.morrox.fontinator.FontEditText
            android:id="@+id/booking_phone_text"
            android:layout_width="360dp"
            android:layout_height="37dp"
            android:background = "@drawable/regular_state"
            android:layout_below="@+id/booking_personnel_text"
            android:layout_toRightOf="@+id/booking_phone_title"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:singleLine="true"
            android:inputType="phone"
            android:text=""
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Regular.otf" />

        <de.morrox.fontinator.FontTextView
            android:id="@+id/number_title"
            android:layout_width="154dp"
            android:layout_height="37dp"
            android:layout_below="@+id/booking_phone_title"
            android:background = "@drawable/regular_state"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:text="인원"
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Medium.otf" />
        <de.morrox.fontinator.FontEditText
            android:id="@+id/number_text"
            android:layout_width="360dp"
            android:layout_height="37dp"
            android:background = "@drawable/regular_state"
            android:layout_below="@+id/booking_phone_text"
            android:layout_toRightOf="@+id/number_title"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:singleLine="true"
            android:inputType="number"
            android:text=""
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Regular.otf" />

        <de.morrox.fontinator.FontTextView
            android:id="@+id/program_title"
            android:layout_width="154dp"
            android:layout_height="37dp"
            android:layout_below="@+id/number_title"
            android:background = "@drawable/regular_state"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:text="프로그램명"
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Medium.otf" />
        <de.morrox.fontinator.FontEditText
            android:id="@+id/program_text"
            android:layout_width="360dp"
            android:layout_height="37dp"
            android:background = "@drawable/regular_state"
            android:layout_below="@+id/number_text"
            android:layout_toRightOf="@+id/program_title"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:singleLine="true"
            android:inputType="text"
            android:text=""
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Regular.otf" />

        <de.morrox.fontinator.FontTextView
            android:id="@+id/etc_title"
            android:layout_width="154dp"
            android:layout_height="37dp"
            android:layout_below="@+id/program_title"
            android:background = "@drawable/regular_state"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:text="비고"
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Medium.otf" />
        <de.morrox.fontinator.FontEditText
            android:id="@+id/etc_text"
            android:layout_width="360dp"
            android:layout_height="37dp"
            android:background = "@drawable/regular_state"
            android:layout_below="@+id/program_text"
            android:layout_toRightOf="@+id/etc_title"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:singleLine="true"
            android:inputType="text"
            android:text=""
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Regular.otf" />

        <de.morrox.fontinator.FontTextView
            android:id="@+id/email_title"
            android:layout_width="154dp"
            android:layout_height="37dp"
            android:layout_below="@+id/etc_title"
            android:background = "@drawable/regular_state"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:text="이메일"
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Medium.otf" />
        <de.morrox.fontinator.FontEditText
            android:id="@+id/email_text"
            android:layout_width="360dp"
            android:layout_height="37dp"
            android:background = "@drawable/regular_state"
            android:layout_below="@+id/etc_text"
            android:layout_toRightOf="@+id/email_title"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:singleLine="true"
            android:inputType="textEmailAddress"
            android:text=""
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Regular.otf" />

        <de.morrox.fontinator.FontTextView
            android:id="@+id/team_number_title"
            android:layout_width="154dp"
            android:layout_height="37dp"
            android:layout_below="@+id/email_title"
            android:background = "@drawable/regular_state"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:text="조(그룹)수"
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Medium.otf" />
        <de.morrox.fontinator.FontEditText
            android:id="@+id/team_number_text"
            android:layout_width="360dp"
            android:layout_height="37dp"
            android:background = "@drawable/regular_state"
            android:layout_below="@+id/email_text"
            android:layout_toRightOf="@+id/team_number_title"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:singleLine="true"
            android:inputType="number"
            android:text=""
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Regular.otf" />
        <de.morrox.fontinator.FontTextView
            android:id="@+id/team_person_title"
            android:layout_width="154dp"
            android:layout_height="37dp"
            android:layout_below="@+id/team_number_title"
            android:background = "@drawable/regular_state"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:text="참여자"
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Medium.otf" />
        <de.morrox.fontinator.FontEditText
            android:id="@+id/team_person_text"
            android:layout_width="360dp"
            android:layout_height="37dp"
            android:background = "@drawable/regular_state"
            android:layout_below="@+id/team_number_text"
            android:layout_toRightOf="@+id/team_person_title"
            android:gravity="center_horizontal|left"
            android:paddingLeft="20dp"
            android:includeFontPadding="false"
            android:singleLine="true"
            android:inputType="text"
            android:text=""
            android:textColor="@android:color/black"
            android:textSize="20sp"
            app:font="NotoSansCJKkr_Regular.otf" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="31dp"
            android:layout_below="@+id/team_person_title">

            <de.morrox.fontinator.FontButton
                android:id="@+id/update_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@mipmap/btn_revise"
                android:text="수정"
                android:textColor="@android:color/white"
                android:textSize="20sp"
                app:font="NotoSansCJKkr_Medium.otf"
                android:visibility="visible"/>

            <de.morrox.fontinator.FontButton
                android:id="@+id/save_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/update_button"
                android:text="저장"
                android:textColor="@android:color/white"
                android:textSize="20sp"
                android:background="@mipmap/btn_signup"
                app:font="NotoSansCJKkr_Medium.otf"
                android:visibility="visible"/>
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>
