<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:sl="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/checkbox_layout"
        android:layout_width="72dp"
        android:layout_height="39dp"
        android:layout_centerVertical="true"
        android:background = "@drawable/regular_state">
        <CheckBox
            android:id="@+id/checkbox"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:button="@drawable/toggle_booking_checkbox"/>
    </RelativeLayout>

    <de.morrox.fontinator.FontTextView
        android:id="@+id/date_text"
        android:layout_width="162dp"
        android:layout_height="39dp"
        android:layout_centerVertical="true"
        android:gravity="center"
        android:includeFontPadding="false"
        android:layout_toRightOf="@+id/checkbox_layout"
        android:background = "@drawable/regular_state"
        android:text="2017.09.14"
        android:textSize="20sp"
        app:font="NotoSansCJKkr_Regular.otf"
        android:textColor="@android:color/black"/>

    <de.morrox.fontinator.FontTextView
        android:id="@+id/team_text"
        android:layout_width="148dp"
        android:layout_height="39dp"
        android:layout_centerVertical="true"
        android:gravity="center"
        android:includeFontPadding="false"
        android:layout_toRightOf="@+id/date_text"
        android:background = "@drawable/regular_state"
        android:text="삼성기획실"
        android:textSize="20sp"
        app:font="NotoSansCJKkr_Regular.otf"
        android:textColor="@android:color/holo_blue_dark"/>

    <de.morrox.fontinator.FontTextView
        android:id="@+id/bookings_name_text"
        android:layout_width="148dp"
        android:layout_height="39dp"
        android:layout_centerVertical="true"
        android:gravity="center"
        android:includeFontPadding="false"
        android:layout_toRightOf="@+id/team_text"
        android:background = "@drawable/regular_state"
        android:text="홍길동"
        android:textSize="20sp"
        app:font="NotoSansCJKkr_Regular.otf"
        android:textColor="@android:color/black"/>

    <de.morrox.fontinator.FontTextView
        android:id="@+id/bookings_phone_text"
        android:layout_width="255dp"
        android:layout_height="39dp"
        android:layout_centerVertical="true"
        android:gravity="center"
        android:includeFontPadding="false"
        android:layout_toRightOf="@+id/bookings_name_text"
        android:background = "@drawable/regular_state"
        android:text="010-1234-5678"
        android:textSize="20sp"
        app:font="NotoSansCJKkr_Regular.otf"
        android:textColor="@android:color/black"/>

    <de.morrox.fontinator.FontTextView
        android:id="@+id/bookings_personnel_text"
        android:layout_width="81dp"
        android:layout_height="39dp"
        android:layout_centerVertical="true"
        android:gravity="center"
        android:includeFontPadding="false"
        android:layout_toRightOf="@+id/bookings_phone_text"
        android:background = "@drawable/regular_state"
        android:text="6"
        android:textSize="20sp"
        app:font="NotoSansCJKkr_Regular.otf"
        android:textColor="@android:color/black"/>

    <de.morrox.fontinator.FontTextView
        android:id="@+id/pragram_name_text"
        android:layout_width="174dp"
        android:layout_height="39dp"
        android:layout_centerVertical="true"
        android:gravity="center"
        android:includeFontPadding="false"
        android:layout_toRightOf="@+id/bookings_personnel_text"
        android:background = "@drawable/regular_state"
        android:text="체험하기"
        android:textSize="20sp"
        app:font="NotoSansCJKkr_Regular.otf"
        android:textColor="@android:color/black"/>

    <de.morrox.fontinator.FontTextView
        android:id="@+id/etc_text"
        android:layout_width="157dp"
        android:layout_height="39dp"
        android:layout_centerVertical="true"
        android:gravity="center"
        android:includeFontPadding="false"
        android:layout_toRightOf="@+id/pragram_name_text"
        android:background = "@drawable/regular_state"
        android:text=""
        android:textSize="20sp"
        app:font="NotoSansCJKkr_Regular.otf"
        android:textColor="@android:color/black"/>

</RelativeLayout>