<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/score_main_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="artech.com.arcam.score.ScoreActivity"
    tools:showIn="@layout/app_bar_score">
    <RelativeLayout
        android:id="@+id/score_left_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/auto_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            android:button="@null"
            android:drawableRight="@drawable/auto_radio_button"
            android:drawablePadding="8dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="29dp"
            android:gravity="center"
            android:includeFontPadding="false"
            android:lineSpacingExtra="0dp"
            android:text="@string/score_auto_mode"
            android:textColor="@android:color/black"
            android:textSize="18sp"
            app:font="NotoSansCJKkr_Medium.otf" />

        <RelativeLayout
            android:id="@+id/camera_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="7dp"
            android:layout_marginLeft="28dp"
            android:layout_below="@+id/auto_check"
            android:background="@mipmap/img_screen">

            <ProgressBar
                android:id="@+id/camera_progressBar"
                style="?android:attr/progressBarStyle"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:visibility="gone"/>

            <FrameLayout
                android:id="@+id/archery_video_view"
                android:layout_width="584px"
                android:layout_height="574px"
                android:padding="5dp"></FrameLayout>

            <ImageView
                android:id="@+id/archery_image"
                android:layout_width="584px"
                android:layout_height="574px"
                android:padding="5dp"
                android:visibility="gone"/>

            <artech.com.arcam.utility.TouchView
                android:id="@+id/archery_video_touch_view"
                android:layout_width="584px"
                android:layout_height="574px"
                android:padding="5dp"></artech.com.arcam.utility.TouchView>



            <artech.com.arcam.utility.AutoScoreView
                android:id="@+id/archery_auto_score_result_view"
                android:layout_width="275dp"
                android:layout_height="200dp"></artech.com.arcam.utility.AutoScoreView>

            <artech.com.arcam.utility.AutoScoreView
                android:id="@+id/archery_auto_score_result2_view"
                android:layout_width="275dp"
                android:layout_height="200dp"
                android:layout_toEndOf="@+id/archery_auto_score_result_view"></artech.com.arcam.utility.AutoScoreView>

            <artech.com.arcam.utility.AutoScoreView
                android:id="@+id/archery_auto_score_result3_view"
                android:layout_width="275dp"
                android:layout_height="200dp"
                android:layout_toEndOf="@+id/archery_auto_score_result_view"
                android:layout_below="@+id/archery_auto_score_result2_view"></artech.com.arcam.utility.AutoScoreView>
            <de.morrox.fontinator.FontTextView
                android:id="@+id/video_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="28dp"
                android:text="@string/score_auto_focus_text"
                android:textColor="@android:color/black"
                android:textSize="14sp"
                app:font="NotoSansCJKkr_Regular.otf" />

            <ImageView
                android:id="@+id/play_img"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/img_playbutton"/>
            
            <RelativeLayout
                android:layout_width="562dp"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/archery_video_view"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="12dp"
                android:background="@mipmap/img_playerbar">

                <ImageButton
                    android:id="@+id/zoomin_button"
                    android:layout_width="50dp"
                    android:layout_height="46dp"
                    app:srcCompat="@mipmap/ic_zoomin"
                    android:background="@android:color/transparent"/>
                <ImageButton
                    android:id="@+id/zoomout_button"
                    android:layout_width="50dp"
                    android:layout_height="46dp"
                    android:layout_toEndOf="@+id/zoomin_button"
                    app:srcCompat="@mipmap/ic_zoomout"
                    android:background="@android:color/transparent"/>
                <ImageButton
                    android:id="@+id/fullscreen_button"
                    android:layout_width="50dp"
                    android:layout_height="46dp"
                    android:layout_alignParentRight="true"
                    app:srcCompat="@mipmap/ic_fullscreen"
                    android:background="@android:color/transparent"/>


                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true">
                    <de.morrox.fontinator.FontTextView
                        android:id="@+id/arrow_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:includeFontPadding="false"
                        android:layout_marginRight="30dp"
                        android:text="Arrow 00"
                        android:textColor="@android:color/white"
                        android:textSize="22sp"
                        app:font="NotoSansCJKkr_Medium.otf" />
                    <de.morrox.fontinator.FontTextView
                        android:id="@+id/total_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toEndOf="@id/arrow_text"
                        android:layout_toRightOf="@id/arrow_text"
                        android:includeFontPadding="false"
                        android:text="Total 000"
                        android:textColor="@android:color/white"
                        android:textSize="22sp"
                        app:font="NotoSansCJKkr_Medium.otf" />
                </RelativeLayout>
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/score_right_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/score_left_layout"
        android:layout_marginTop="21dp"
        android:layout_marginLeft="16dp">
        <RelativeLayout
            android:id="@+id/score_board_tab_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <de.morrox.fontinator.FontButton
                android:id="@+id/tab_round_1_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@mipmap/tab_active"
                android:text="1R"
                android:textAllCaps="false"
                android:textColor="@color/bluegrayfont"
                android:textSize="16sp"
                app:font="Roboto_Regular.otf" />
            <de.morrox.fontinator.FontButton
                android:id="@+id/tab_round_2_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@mipmap/tab_non"
                android:layout_toEndOf="@+id/tab_round_1_button"
                android:layout_marginLeft="1dp"
                android:text="2R"
                android:textAllCaps="false"
                android:textColor="@color/bluegrayfont"
                android:textSize="16sp"
                app:font="Roboto_Regular.otf" />
            <de.morrox.fontinator.FontButton
                android:id="@+id/tab_round_3_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@mipmap/tab_non"
                android:layout_toEndOf="@+id/tab_round_2_button"
                android:layout_marginLeft="1dp"
                android:text="3R"
                android:textAllCaps="false"
                android:textColor="@color/bluegrayfont"
                android:textSize="16sp"
                app:font="Roboto_Regular.otf" />
            <de.morrox.fontinator.FontButton
                android:id="@+id/tab_round_4_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@mipmap/tab_non"
                android:layout_toEndOf="@+id/tab_round_3_button"
                android:layout_marginLeft="1dp"
                android:text="4R"
                android:textAllCaps="false"
                android:textColor="@color/bluegrayfont"
                android:textSize="16sp"
                app:font="Roboto_Regular.otf" />
            <de.morrox.fontinator.FontButton
                android:id="@+id/tab_round_5_button"
