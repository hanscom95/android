                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@+id/info_weather_text"
                            android:layout_marginLeft="10dp"
                            android:layout_centerVertical="true"
                            android:src="@mipmap/line4"/>

                        <de.morrox.fontinator.FontTextView
                            android:id="@+id/info_wind_text"
                            android:layout_width="120dp"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@+id/info_second_line"
                            android:layout_marginLeft="10dp"
                            android:layout_centerVertical="true"
                            android:gravity="center"
                            android:singleLine="true"
                            android:includeFontPadding="false"
                            android:text="WIND(ONE)"
                            android:textColor="@android:color/white"
                            android:textSize="14sp"
                            app:font="NotoSansCJKkr_Bold.otf" />


                        <ImageView
                            android:id="@+id/info_third_line"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@+id/info_wind_text"
                            android:layout_marginLeft="10dp"
                            android:layout_centerVertical="true"
                            android:src="@mipmap/line4"/>

                        <de.morrox.fontinator.FontTextView
                            android:id="@+id/info_distance_text"
                            android:layout_width="130dp"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@+id/info_third_line"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="10dp"
                            android:gravity="center"
                            android:singleLine="true"
                            android:includeFontPadding="false"
                            android:text="DISTANCE(ONE)"
                            android:textColor="@android:color/white"
                            android:textSize="14sp"
                            app:font="NotoSansCJKkr_Bold.otf" />
                    </RelativeLayout>


                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/info_first_layout"
                        android:layout_marginTop="3dp"
                        android:layout_marginLeft="156dp"
                        android:layout_marginBottom="7dp">
                        <RelativeLayout
