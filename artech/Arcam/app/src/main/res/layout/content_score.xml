                android:gravity="center"
                android:background="@drawable/toggle_archery_number_ten_button"
                android:button="@android:color/transparent" />
            <de.morrox.fontinator.FontCheckBox
                android:id="@+id/arrow_top_left_check"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/number_10_check"
