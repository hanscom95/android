                            android:background="@mipmap/img_info_non_press">
                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_centerHorizontal="true">
                                <de.morrox.fontinator.FontTextView
                                    android:id="@+id/brace_high_text"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:includeFontPadding="false"
                                    android:text="@string/score_brace_high"
                                    android:textColor="@android:color/white"
                                    android:textSize="14sp"
                                    app:font="NotoSansCJKkr_Bold.otf" />

                                <de.morrox.fontinator.FontTextView
                                    android:id="@+id/brace_high_value_text"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:layout_toEndOf="@+id/brace_high_text"
                                    android:includeFontPadding="false"
                                    android:maxLength="6"
                                    android:layout_marginLeft="5dp"
                                    android:text="000"
                                    android:textColor="@android:color/white"
                                    android:textSize="14sp"
                                    app:font="NotoSansCJKkr_Bold.otf" />
                            </RelativeLayout>
                        </RelativeLayout>
                        <RelativeLayout
                            android:id="@+id/tiller_high_layout"
                            android:layout_width="139dp"
                            android:layout_height="33dp"
                            android:layout_toEndOf="@+id/brace_high_layout"
                            android:layout_alignTop="@+id/brace_high_layout"
                            android:layout_marginLeft="14dp"
                            android:background="@mipmap/img_info_non_press">
                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_centerHorizontal="true">

                                <de.morrox.fontinator.FontTextView
                                    android:id="@+id/tiller_high_text"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:includeFontPadding="false"
                                    android:text="@string/score_tiller_high"
                                    android:textColor="@android:color/white"
