                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/number_4_check"
                android:layout_toEndOf="@+id/number_4_check"
                android:gravity="center"
                android:background="@drawable/toggle_archery_number_five_button"
