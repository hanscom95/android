<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_score_popup"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    android:focusable="true"
    android:focusableInTouchMode="true">
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp">
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/number_7_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:background="@drawable/toggle_archery_number_seven_button"
            android:button="@android:color/transparent" />
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/number_8_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/number_7_check"
            android:layout_toEndOf="@+id/number_7_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_number_eight_button"
            android:button="@android:color/transparent" />
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/number_9_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/number_8_check"
            android:layout_toEndOf="@+id/number_8_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_number_nine_button"
            android:button="@android:color/transparent" />
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/number_10_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/number_9_check"
            android:layout_toEndOf="@+id/number_9_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_number_ten_button"
            android:button="@android:color/transparent" />
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/arrow_top_left_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/number_10_check"
            android:layout_toEndOf="@+id/number_10_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_arrow_top_left_button"
            android:button="@android:color/transparent" />
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/arrow_top_right_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/arrow_top_left_check"
            android:layout_toEndOf="@+id/arrow_top_left_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_arrow_top_right_button"
            android:button="@android:color/transparent" />


        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/number_4_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/number_7_check"
            android:layout_below="@+id/number_7_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_number_four_button"
            android:button="@android:color/transparent" />
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/number_5_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/number_4_check"
            android:layout_toEndOf="@+id/number_4_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_number_five_button"
            android:button="@android:color/transparent" />
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/number_6_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/number_5_check"
            android:layout_toEndOf="@+id/number_5_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_number_six_button"
            android:button="@android:color/transparent" />
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/number_xten_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/number_6_check"
            android:layout_toEndOf="@+id/number_6_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_number_xten_button"
            android:button="@android:color/transparent" />
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/arrow_top_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/number_xten_check"
            android:layout_toEndOf="@+id/number_xten_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_arrow_top_button"
            android:button="@android:color/transparent" />
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/arrow_down_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/arrow_top_check"
            android:layout_toEndOf="@+id/arrow_top_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_arrow_down_button"
            android:button="@android:color/transparent" />


        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/number_1_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/number_4_check"
            android:layout_below="@+id/number_4_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_number_one_button"
            android:button="@android:color/transparent" />
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/number_2_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/number_1_check"
            android:layout_toEndOf="@+id/number_1_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_number_two_button"
            android:button="@android:color/transparent" />
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/number_3_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/number_2_check"
            android:layout_toEndOf="@+id/number_2_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_number_three_button"
            android:button="@android:color/transparent" />
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/arrow_left_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/arrow_top_check"
            android:layout_below="@+id/arrow_top_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_arrow_left_button"
            android:button="@android:color/transparent" />
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/arrow_right_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/arrow_left_check"
            android:layout_toEndOf="@+id/arrow_left_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_arrow_right_button"
            android:button="@android:color/transparent" />


        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/number_0_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/number_1_check"
            android:layout_below="@+id/number_1_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_number_miss_button"
            android:button="@android:color/transparent" />
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/arrow_down_left_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/arrow_left_check"
            android:layout_below="@+id/arrow_left_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_arrow_down_left_button"
            android:button="@android:color/transparent" />
        <de.morrox.fontinator.FontCheckBox
            android:id="@+id/arrow_down_right_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/arrow_down_left_check"
            android:layout_toEndOf="@+id/arrow_down_left_check"
            android:gravity="center"
            android:background="@drawable/toggle_archery_arrow_down_right_button"
            android:button="@android:color/transparent" />


        <de.morrox.fontinator.FontButton
            android:id="@+id/save_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/arrow_top_right_check"
            android:layout_toEndOf="@+id/arrow_top_right_check"
            android:background="@drawable/toggle_score_button"
            android:text="@string/score_save_button"
            android:textAllCaps="false"
            android:textColor="@android:color/white"
            android:textSize="25sp"
            app:font="NotoSansCJKkr_Bold.otf" />

        <de.morrox.fontinator.FontButton
            android:id="@+id/cancel_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/save_button"
            android:layout_below="@+id/save_button"
            android:layout_marginTop="2dp"
            android:background="@drawable/toggle_score_button"
            android:text="@string/score_cancel_button"
            android:textAllCaps="false"
            android:textColor="@android:color/white"
            android:textSize="25sp"
            app:font="NotoSansCJKkr_Bold.otf" />
    </RelativeLayout>
</RelativeLayout>
