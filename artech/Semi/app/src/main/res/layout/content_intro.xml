<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/intro_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="artech.com.semi.login.IntroActivity"
    tools:showIn="@layout/activity_intro"
    android:background="@mipmap/bg_intro">
    <ImageView
        android:id="@+id/hookImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:src="@mipmap/img_hook"/>

    <!--<com.kakao.usermgmt.LoginButton
        android:id="@+id/kakao_real_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="10dp"
        android:visibility="gone"/>-->

    <ImageView
        android:id="@+id/subTitleImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/hookImg"
        android:layout_marginTop="@dimen/intro_sub_title_top_margin"
        android:src="@mipmap/intro_sub_title"/>

    <ImageView
        android:id="@+id/mainTitleImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/subTitleImg"
        android:layout_marginTop="@dimen/intro_main_title_top_margin"
        android:src="@mipmap/intro_title"/>

    <ImageView
        android:id="@+id/logoImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/intro_logo_bottom_margin"
        android:src="@mipmap/img_logo"/>

</RelativeLayout>
