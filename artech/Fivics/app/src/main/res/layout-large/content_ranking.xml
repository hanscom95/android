                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@mipmap/myrank">

                    <RelativeLayout
                        android:id="@+id/my_rank_column"
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true">

                        <de.morrox.fontinator.FontTextView
                            android:id="@+id/my_rank_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="10dp"
                            android:text="-"
                            android:textColor="@android:color/white"
                            android:textSize="32sp"
                            app:font="NotoSansCJKkr_Bold.otf" />

                        <ImageView
                            android:id="@+id/my_ranking_medal_img"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/medal_1"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:visibility="gone"/>
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/my_player_column"
                        android:layout_width="510dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toEndOf="@+id/my_rank_column">


                        <ImageView
                            android:id="@+id/my_player_member_img"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="100dp"
                            android:src="@mipmap/img_member"
                            android:visibility="visible" />

                        <de.morrox.fontinator.FontTextView
                            android:id="@+id/my_player_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="26dp"
                            android:layout_toEndOf="@+id/my_player_member_img"
                            android:text="HAHA(5662)"
                            android:textColor="@android:color/white"
                            android:textSize="32sp"
                            app:font="NotoSansCJKkr_Bold.otf" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/my_avg_column"
                        android:layout_width="350dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toEndOf="@+id/my_player_column">

                        <de.morrox.fontinator.FontTextView
                            android:id="@+id/my_avg_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="10dp"
                            android:text="0.0"
                            android:textColor="@android:color/white"
                            android:textSize="32sp"
                            app:font="NotoSansCJKkr_Bold.otf" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/my_total_column"
                        android:layout_width="310dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toEndOf="@+id/my_avg_column">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true">

                            <de.morrox.fontinator.FontTextView
                                android:id="@+id/my_total_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0"
                                android:textColor="@android:color/white"
                                android:textSize="32sp"
                                app:font="NotoSansCJKkr_Bold.otf" />

                            <de.morrox.fontinator.FontTextView
                                android:id="@+id/my_arrow_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dp"
                                android:layout_toEndOf="@+id/my_total_text"
                                android:text="(0)"
                                android:textColor="@android:color/holo_green_light"
                                android:textSize="32sp"
                                app:font="NotoSansCJKkr_Regular.otf" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/my_homeshop_column"
                        android:layout_width="500dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toEndOf="@+id/my_total_column">

                        <de.morrox.fontinator.FontTextView
                            android:id="@+id/my_homeshop_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="10dp"
                            android:text="수원권선점"
                            android:textColor="@android:color/white"
                            android:textSize="32sp"
                            app:font="NotoSansCJKkr_Bold.otf" />
                    </RelativeLayout>
                </RelativeLayout>

            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/button_bottom_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:paddingBottom="58dp">


        <RelativeLayout
            android:id="@+id/before_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/before_img"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="57dp"
                android:src="@mipmap/ic_arrow_back_grayblue" />

            <de.morrox.fontinator.FontTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="16dp"
                android:layout_toEndOf="@+id/before_img"
                android:text="@string/before_en_button"
                android:textSize="30sp"
                app:font="NotoSansCJKkr_Bold.otf"
                android:textColor="@color/archery_grey" />
        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/next_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:visibility="visible">

            <de.morrox.fontinator.FontTextView
                android:id="@+id/next_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="16dp"
                android:text="@string/finish_en_button"
                android:textColor="@color/prohibition_text"
                android:textSize="30sp"
                app:font="NotoSansCJKkr_Bold.otf" />

            <ImageView
                android:id="@+id/next_img"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_marginRight="59dp"
                android:layout_toEndOf="@+id/next_button"
                android:src="@mipmap/ic_arrow_next_red" />
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/my_detail_history_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="67dp"
        android:background="@mipmap/ranking_bottom">

        <de.morrox.fontinator.FontTextView
            android:id="@+id/my_detail_history_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/ranking_my_detail_history"
            android:textColor="@color/archery_board_select"
            android:textSize="36sp"
            app:font="NotoSansCJKkr_Medium.otf" />
    </RelativeLayout>

</RelativeLayout>
