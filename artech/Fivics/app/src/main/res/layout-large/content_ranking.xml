                            android:textSize="32sp"
                            app:font="NotoSansCJKkr_Regular.otf" />
                    </RelativeLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/homeshop_number_one_column"
                    android:layout_width="500dp"
                    android:layout_height="wrap_content"
